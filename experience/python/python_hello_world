class NguoiNoiTieng:
    def __init__(self, name, age, field):
        self.name = name
        self.age = age
        self.field = field
    def debut(self):
        """ now begin be well-known """
        return "now %s start to be popular person in %s field at %d" %(self.name, self.field, self.age)
if __name__ == "__main__":
    hurrykng = NguoiNoiTieng("hurrykng", 23, "rap")
    print (hurrykng.debut ())